{"components/dh-custom/products/description-tabs":"{{#if product.description}}\r\n<div class=\"toggle-block\">\r\n    <h6 class=\"toggle-title\">Details\r\n        <div class=\"toggle-title-icon\">\r\n            <i class=\"icon add\"><svg><use xlink:href=\"#icon-accordion-plus\"></use></svg></i>\r\n            <i class=\"icon remove\"><svg><use xlink:href=\"#icon-accordion-minus\"></use></svg></i>\r\n        </div>\r\n    </h6>\r\n    <div class=\"toggle-content\">\r\n        {{{product.description}}}\r\n    </div>\r\n</div>\r\n{{/if}}\r\n\r\n{{#if product.warranty}}\r\n<div class=\"toggle-block\">\r\n    <h6 class=\"toggle-title\">Specifications\r\n        <div class=\"toggle-title-icon\">\r\n            <i class=\"icon add\"><svg><use xlink:href=\"#icon-accordion-plus\"></use></svg></i>\r\n            <i class=\"icon remove\"><svg><use xlink:href=\"#icon-accordion-minus\"></use></svg></i>\r\n        </div>\r\n    </h6>\r\n    <div class=\"toggle-content\">\r\n        {{{product.warranty}}}\r\n    </div>\r\n</div>\r\n{{/if}}\r\n\r\n{{#if product.videos.list.length}}\r\n<div class=\"toggle-block\">\r\n    <h6 class=\"toggle-title\">{{lang 'products.videos.header'}}\r\n        <div class=\"toggle-title-icon\">\r\n            <i class=\"icon add\"><svg><use xlink:href=\"#icon-accordion-plus\"></use></svg></i>\r\n            <i class=\"icon remove\"><svg><use xlink:href=\"#icon-accordion-minus\"></use></svg></i>\r\n        </div>\r\n    </h6>\r\n    <div class=\"toggle-content\" id=\"videoGallery-content\">\r\n        {{#if product.videos.list.length}}\r\n            {{> components/dh-custom/products/videos product.videos}}\r\n        {{/if}}\r\n    </div>\r\n</div>\r\n{{/if}}\r\n","components/dh-custom/products/videos":"<div class=\"toggle-content-custom\" id=\"videoGallery-content\" data-video-gallery>\r\n    <div class=\"videoGallery-main\">\r\n        <iframe\r\n            id=\"player\"\r\n            class=\"lazyload\"\r\n            type=\"text/html\"\r\n            width=\"640\"\r\n            height=\"390\"\r\n            frameborder=\"0\"\r\n            webkitAllowFullScreen\r\n            mozallowfullscreen\r\n            allowFullScreen\r\n            data-src=\"//www.youtube.com/embed/{{this.featured.id}}?rel=0\"\r\n            data-video-player>\r\n        </iframe>\r\n    </div>\r\n    <ul class=\"videoGallery-list\">\r\n    {{#each this.list}}\r\n        <li class=\"videoGallery-item\">\r\n            <a href=\"#\" class=\"video {{#if @first}}is-active{{/if}}\" data-video-item data-video-id=\"{{id}}\">\r\n                <div class=\"video-figure\">\r\n                    <img class=\"lazyload\" data-sizes=\"auto\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"//i.ytimg.com/vi/{{id}}/default.jpg\"/>\r\n                </div>\r\n                <div class=\"video-body\">\r\n                    <h5 class=\"video-title\">{{title_long}}</h5>\r\n                    <p class=\"video-description\">{{description_short}}</p>\r\n                </div>\r\n            </a>\r\n        </li>\r\n    {{/each}}\r\n    </ul>\r\n</div>\r\n"}
