{"components/dh-custom/common/wishlist-dropdown":"<form action=\"{{product.add_to_wishlist_url}}\" class=\"form form-wishlist\" data-wishlist-add method=\"post\">\r\n\r\n    <ul aria-hidden=\"true\" class=\"dropdown-menu\" data-dropdown-content id=\"wishlist-dropdown\" tabindex=\"-1\">\r\n        {{#if customer.wishlists}} {{#each customer.wishlists}}\r\n        <li>\r\n            <span class=\"button--with--span\" data-hover=\"{{this.name}}\">\r\n                {{this.name}}\r\n                <input class=\"button1 {{#if this.num_items '!==' '0'}}button--has-items{{/if}}\" formaction=\"{{this.add_url}}&product_id={{../product.id}}\"\r\n                type=\"submit\" value=\"\">\r\n            </span>\r\n        </li>\r\n        {{/each}} {{else}}\r\n        <li>\r\n            <input class=\"button\" type=\"submit\" data-hover=\"{{lang 'account.wishlists.add_to_default'}}\" value=\"{{lang 'account.wishlists.add_to_default'}}\">\r\n        </li>\r\n        {{/if}}\r\n        {{#if customer}}\r\n        <li>\r\n            <a data-wishlist class=\"button\" href=\"/wishlist.php?action=addwishlist&product_id={{product.id}}\">{{lang 'account.wishlists.create_new'}}</a>\r\n        </li>\r\n        {{else}}\r\n        <li>\r\n          <a class=\"button\" href=\"/wishlist.php\">{{lang 'account.wishlists.create_new'}}</a>\r\n        </li>\r\n        {{/if}}\r\n    </ul>\r\n</form>\r\n"}
