{"components/dh-custom/common/navigation-mega-menu":"<ul class=\"mega-menu-wrap\">\r\n    <li class=\"clearfix\">\r\n      <div class=\"mega-menu\">\r\n        <div class=\"mega-menu-level-first-wrap\">\r\n          <ul class=\"mega-menu-level-first clearfix\">\r\n            {{#each children}}\r\n            <li class=\"{{#if @index '==' '0'}}active{{/if}} category-item\">\r\n              {{#if children}}\r\n              <a class=\"needsclick\" href=\"{{url}}\">{{name}} \r\n                  <svg><use xlink:href=\"#icon-slide-arrow-right\"></use></svg>\r\n                </a>\r\n              <ul class=\"mega-menu-second-wrap\">\r\n                <div class=\"mega-menu-second-wrap-inner\">\r\n                  {{#each children}}\r\n                  <div class=\"mega-menu-second-item\">\r\n                    {{#if children}}\r\n                      {{#each children}}\r\n                          <p class=\"{{#if @last}}last-child{{/if}}\">\r\n                              <a href=\"{{url}}\">{{name}}</a>\r\n                          </p>\r\n                      {{/each}}\r\n                      {{#if children}}\r\n                          {{#each children}}\r\n                              <p class=\"{{#if @last}}last-child{{/if}}\">\r\n                                  <a href=\"{{url}}\">{{name}}</a>\r\n                              </p>\r\n                          {{/each}}\r\n                      {{/if}}\r\n                    {{else}}\r\n                    <p class=\"cat-heading\"><a href=\"{{url}}\">{{name}}</a></p>\r\n                    {{/if}}\r\n                  </div>\r\n                  {{/each}}\r\n                </div>\r\n              </ul>\r\n              {{else}}\r\n              <a  href=\"{{url}}\">{{name}}</a>\r\n              {{/if}}\r\n            </li>\r\n            {{/each}}\r\n          </ul>\r\n        </div>\r\n      </div>\r\n        {{#if image}}\r\n            <div class=\"category-image\">\r\n                <div class=\"category-img-block\">\r\n                    <img src=\"{{getImage image \"thumbnail\"}}\">\r\n                </div>\r\n                {{#if (dashcase name)  \"==\" 'lawn-landscape-pest-control-equipment'}}\r\n                <a class=\"button\" href=\"{{url}}\">SHOP ALL LAWN & GARDEN</a>\r\n                {{else}}\r\n                <a class=\"button\" href=\"{{url}}\">SHOP ALL {{name}}</a>\r\n                {{/if}}                \r\n            </div>\r\n        {{/if}}\r\n    </li>\r\n  </ul>\r\n  \r\n\r\n   \r\n \r\n  \r\n"}
