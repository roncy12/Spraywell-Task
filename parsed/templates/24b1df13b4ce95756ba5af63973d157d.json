n                                        {{#if children}}\r\n                                            {{#each children}}\r\n                                                {{#if children}}\r\n                                                {{#if name '==' 'Parts & Accessories'}}\r\n                                                <li  class=\"category-item category-parent\"><a href=\"{{url}}\"><strong>Pressure Washers <br> {{name}}</strong></a></li>\r\n                                                    {{#each children}}\r\n                                                        <li class=\"category-item\"><a href=\"{{url}}\">{{name}}</a></li>\r\n                                                    {{/each}}\r\n                                                {{/if}}\r\n                                                {{/if}}\r\n                                            {{/each}}\r\n                                        {{/if}}\r\n                                    {{/if}}\r\n                        {{/each}}\r\n                        </ul>\r\n                </div>\r\n            </ul>\r\n        </li>\r\n\r\n        {{#each categories}}\r\n            {{#if (dashcase name) '==' 'lawn-landscape-pest-control-equipment'}}\r\n            <li class=\"navPages-item {{#if is_active '==' true}}ActivePage{{/if}} mega-menu-item {{dashcase name}}\">\r\n                {{#if children}}\r\n                    <a class=\"navPages-action needsclick\" href=\"{{url}}\">{{name}}<i class=\"icon\"><svg><use xlink:href=\"#icon-menu-arrow-grey\"></use></svg></i></a>\r\n                    {{> components/dh-custom/common/navigation-mega-menu}}\r\n                {{else}}\r\n                    <a class=\"navPages-action needsclick\" href=\"{{url}}\">\r\n                        {{name}}\r\n                    </a>\r\n                {{/if}}\r\n            </li>\r\n            {{/if}}\r\n        {{/each}}\r\n\r\n        \r\n\r\n        {{#if shop_by_brand}}\r\n            <li class=