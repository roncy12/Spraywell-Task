{"components/dh-custom/common/navigation-mega-menu-aoc":"<ul class=\"mega-menu-wrap\">\r\n    <li class=\"clearfix\">\r\n      <div class=\"mega-menu custom_mega_menu\">\r\n        <div class=\"mega-menu-level-first-wrap custom_level_first_wrap\">\r\n        <div class=\"heading-section\">\r\n            <h3 class=\"page-heading\">{{name}}</h3>\r\n            <a class=\"button\" href=\"{{url}}\">VIEW ALL</a>\r\n        </div>    \r\n\r\n          <ul class=\"mega-menu-level-first clearfix custom_mega_menu {{#gt (length children) 6}}slick_menu{{/gt}}\">\r\n            {{#each children}}\r\n              {{#unless name '==' 'Parts & Accessories'}}\r\n                <li class=\"{{#if @index '==' '0'}}active{{/if}} category-item\">\r\n                  <a  href=\"{{url}}\"> <img class=\"lazyload\" src=\"{{cdn (concat 'https://store-oskkn9ejod.mybigcommerce.com/content/navigation_img/' (concat  (lowercase (dashcase name)) '.png'))}}\"  alt=\"{{name}}\"></a>\r\n                  <a  href=\"{{url}}\">{{name}}</a>\r\n                </li>\r\n              {{/unless}}\r\n            {{/each}}\r\n          </ul>\r\n        </div>\r\n      </div>\r\n    </li>\r\n  </ul>\r\n  \r\n\r\n   \r\n \r\n  \r\n"}
